    <!-- =-=-=-=-=-=-= Register Form =-=-=-=-=-=-= -->
    <section class="section-padding-80 white" id="register">
      <div class="container">
        <div class="row">
          <div class="col-sm-6 col-sm-offset-3 col-md-6 col-md-offset-3">

            <div class="box-panel" style="box-shadow: 10px 10px 12px grey;">

              

              <!-- form login -->
              <form action="signupauthen" method="get">
                   <div class="form-group">
                       <h1 style="text-align: center">BrainBox</h1>
                       <hr>
                  
                </div>
                <div class="form-group">
                  <label>Name</label>
                  <input type="text" placeholder="Your Full Name" name="uname" class="form-control">
                </div>
                <div class="form-group">
                  <label>Email</label>
                  <input type="email" placeholder="Your Email" name="email" class="form-control">
                </div>
                <div class="form-group">
                  <label>Password</label>
                  <input type="password"  name="pass" placeholder="Your Password" class="form-control">
                </div>
                <div class="form-group">
                  <label>Confirm Password</label>
                  <input type="password" name="cpass" placeholder="Verify Your Password" class="form-control">
                </div>
                   
                <div class="form-group">
                  <div class="row">

                  
                  </div>
                </div>

                <button class="btn btn-primary btn-lg btn-block">Create Account</button>

              </form>
              <!-- form login -->

            </div>
          </div>

          <div class="clearfix"></div>
        </div>
      </div>
      <!-- end container -->
    </section>
